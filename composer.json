{
    "name": "phlib/console-process",
    "type": "library",
    "description": "Console implementation.",
    "keywords": ["symfony", "console", "daemon", "background", "pcntl", "posix"],
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Luke Richards"
        },
        {
            "name": "Phlib Team & Contributors",
            "homepage": "https://github.com/phlib/console-process/contributors"
        }
    ],
    "require": {
        "php": "^5.5|^7",
        "ext-pcntl" : "*",
        "ext-posix" : "*",
        "symfony/console": "^3|^4|^5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "php-mock/php-mock-phpunit": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\ConsoleProcess\\": "src/"
        }
    },
    "autoload-dev": {
       "psr-4": {
           "Phlib\\ConsoleProcess\\Tests\\": "tests/"
       }
    }
}
